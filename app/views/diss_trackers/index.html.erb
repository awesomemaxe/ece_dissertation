<h1>Dissertation tracker</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Last name</th>
      <th>First name</th>
      <th>Titile</th>
      <th>Chair</th>
      <th>Advisor</th>
      <th>Reader2</th>
      <th>Reader3</th>
      <th>Reader4</th>
      <th>Reader5</th>
      <th>Data approved</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @diss_trackers.each do |diss_tracker| %>
      <tr>
        <td><%= diss_tracker.UID %></td>
        <td><%= diss_tracker.last_name %></td>
        <td><%= diss_tracker.first_name %></td>
        <td><%= diss_tracker.titile %></td>
        <td><%= diss_tracker.chair %></td>
        <td><%= diss_tracker.advisor %></td>
        <td><%= diss_tracker.reader2 %></td>
        <td><%= diss_tracker.reader3 %></td>
        <td><%= diss_tracker.reader4 %></td>
        <td><%= diss_tracker.reader5 %></td>
        <td><%= diss_tracker.data_approved %></td>
        <td><%= link_to 'Show', diss_tracker %></td>
        <td><%= link_to 'Edit', edit_diss_tracker_path(diss_tracker) %></td>
        <td><%= link_to 'Destroy', diss_tracker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diss tracker', new_diss_tracker_path %>
